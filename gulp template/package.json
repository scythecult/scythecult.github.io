{
  "name": "gulp-template",
  "version": "1.0.0",
  "description": "modern gulp assembly",
  "main": "index.html",
  "scripts": {
    "editorconfig": "editorconfig-cli",
    "stylelint": "stylelint \"source/less/**/*.less\" --syntax less",
    "test": "npm run editorconfig && npm run stylelint",
    "start": "gulp"
  },
  "browserslist": [
    "last 2 version",
    "not dead",
    "not ie <= 11",
    "iOS >= 12"
  ],
  "editorconfig-cli": [
    "*.json",
    "*.js",
    "source/*.html",
    "source/js/**/*.js",
    "source/less/**/*.less"
  ],
  "author": "Ale Shov",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.12.0",
    "@babel/preset-env": "^7.12.0",
    "@htmlacademy/editorconfig-cli": "^1.0.0",
    "browser-sync": "^2.26.12",
    "del": "^6.0.0",
    "eslint": "^7.11.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-babel": "^8.0.0",
    "gulp-clean-css": "^4.3.0",
    "gulp-file-include": "^2.2.2",
    "gulp-gh-pages": "^0.5.4",
    "gulp-group-css-media-queries": "^1.2.2",
    "gulp-htmlmin": "^5.0.1",
    "gulp-imagemin": "^7.1.0",
    "gulp-less": "^4.0.1",
    "gulp-plumber": "^1.2.1",
    "gulp-rename": "^2.0.0",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-svg-sprite": "^1.5.0",
    "gulp-terser": "^1.4.0",
    "gulp-w3c-html-validator": "^2.0.1",
    "gulp-webp": "^4.0.1",
    "gulp-webp-html": "^1.0.2",
    "stylelint": "^13.7.2",
    "stylelint-config-htmlacademy": "^0.1.4"
  },
  "dependencies": {}
}
